<div class="container">
	<div class="row">
		<div class="well col-md-4 col-md-offset-4 col-xs-12 centered">

			<h4><%= @entry.question %> </h4>

			<%= image_tag(@entry.image, size: '150') %> 

			<p>
				<%= link_to "Yes", vote_stack_entry_path(@stack, @entry, entry_show_time: @entry_show_time, response: true), method: :post %>
				<%= link_to "No", vote_stack_entry_path(@stack, @entry, entry_show_time: @entry_show_time, response: false), method: :post %>
			</p>
		</div>
	</div>
</div>